<tr>
  <td class="col-min">
    <span class="label label-danger"><i class="fa fa-times"></i></span>
  </td>
  <td>
    <strong>{{ log.source.typLabel }}</strong> {{ audit_assoc(log.source) }} {{ 'audit.index.was_removed'|trans }}
    {{ audit_blame(log.blame) }}
  </td>
  <td class="col-min" data-timeago="{{ log.loggedAt|date('c') }}">{{ log.loggedAt|localizeddate('short', 'short') }}</td>
  <td class="col-min">
  </td>
</tr>
