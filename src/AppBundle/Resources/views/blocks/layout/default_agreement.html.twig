        <td style="padding-left: 5px;">{% if not application.dateFrom is empty %}{{ application.dateFrom | date("Y-m-d") }}{% else %}&nbsp;{% endif %}{% if not application.dateTo is empty and not application.dateFrom | date("Y-m-d") == application.dateTo | date("Y-m-d")  %} - {{ application.dateTo | date("Y-m-d") }}{% endif %}</td>
    </tr>
    <tr>
        <td style="width: 100px; padding-left: 5px;">Vieta</td>
